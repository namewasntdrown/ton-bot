{
  "name": "ton-bot-monorepo",
  "private": true,
  "scripts": {
    "bootstrap": "npm --prefix services/bot i && npm --prefix services/api i && npm --prefix services/relayer i",
    "dev": "concurrently \"npm:dev:bot\" \"npm:dev:api\"",
    "dev:bot": "npm --prefix services/bot run dev",
    "dev:api": "npm --prefix services/api run dev"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}